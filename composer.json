{
    "name": "spryker/category-discount-connector",
    "type": "library",
    "description": "CategoryDiscountConnector module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/category": "^5.3.0",
        "spryker/discount": "^9.0.0",
        "spryker/discount-extension": "^1.2.0",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "^3.0.0",
        "spryker/product-category": "^4.13.0",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/product": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
